; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Turn-Based RPG"
run/main_scene="res://MainMenu/MainMenu.tscn"
config/icon="res://Icon.png"

[autoload]

Manager="*res://Managers/Manager.tscn"
PopupManager="*res://Managers/PopupManager/PopupManager.tscn"
TransitionManager="*res://Managers/TransitionManager/TransitionManager.tscn"
DialogManager="*res://Managers/DialogManager/DialogManager.tscn"
InteractionManager="*res://Managers/InteractionManager/InteractionManager.tscn"
BattleManager="*res://Managers/BattleManager/BattleManager.tscn"
ShopManager="*res://Managers/ShopManager/ShopManager.tscn"
Global="*res://Global.cs"

[display]

window/size/width=320
window/size/height=180
window/size/resizable=false
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

